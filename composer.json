{
    "name": "digipolisgent/robo-digipolis-package",
    "description": "General Packaging/Compile tasks for Robo Task Runner",
    "type": "robo-tasks",
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "consolidation/robo": "~1.0",
        "php": ">=5.5.0",
        "pear/archive_tar": "^1.4",
        "symfony/debug": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "DigipolisGent\\Robo\\Task\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigipolisGent\\Tests\\Robo\\Task\\Package\\": "tests"
        }
    },
    "scripts": {
        "test": "robo --ansi test"
    },
