{
    "name": "digipolisgent/robo-digipolis-package",
    "description": "General Packaging/Compile tasks for Robo Task Runner",
    "type": "robo-tasks",
    "minimum-stability": "RC",
    "prefer-stable": true,
    "require": {
        "consolidation/robo": "^3.0 || ^4.0",
        "pear/archive_tar": "^1.4.14",
        "symfony/error-handler": "^6|^5|^4",
        "symfony/process": "^6|5|^4",
        "digipolisgent/robo-digipolis-general": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.20"
    },
    "autoload": {
        "psr-4": {
            "DigipolisGent\\Robo\\Task\\Package\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigipolisGent\\Tests\\Robo\\Task\\Package\\": "tests"
        }
    },
    "license": "MIT",
